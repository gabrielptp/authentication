{
  "name": "authentication",
  "version": "0.0.1",
  "description": "Authentication API with Nest.js and Redis",
  "author": "Gabriel",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "dev": "NODE_ENV=development ts-node src/main.ts",
    "dev:watch": "NODE_ENV=development nodemon --exec \"ts-node src/main.ts\" --watch src --ext ts",
    "start:prod": "NODE_ENV=production node dist/main.js",
    "docker:test": "docker-compose up --build",
    "docker:redis": "docker-compose -f docker-compose.redis.yml up -d",
    "docker:redis:stop": "docker-compose -f docker-compose.redis.yml down",
    "docker:postgres": "docker-compose -f docker-compose.postgres.yml up -d",
    "docker:postgres:stop": "docker-compose -f docker-compose.postgres.yml down",
    "docker:dev": "docker-compose -f docker-compose.redis.yml -f docker-compose.postgres.yml up -d",
    "docker:dev:stop": "docker-compose -f docker-compose.redis.yml -f docker-compose.postgres.yml down",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest --testPathPatterns=spec.ts",
    "test:e2e": "jest --config ./test/jest.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.0",
    "pg": "^8.16.3",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/chai": "^4.3.20",
    "@types/express": "^5.0.0",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.18.8",
    "@types/supertest": "^6.0.2",
    "chai": "^4.5.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.20.0"
  }
}
